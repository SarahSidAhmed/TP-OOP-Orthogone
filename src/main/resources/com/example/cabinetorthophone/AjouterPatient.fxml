            <Label layoutX="311.0" layoutY="197.0" text="Telephone:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tel" layoutX="389.0" layoutY="194.0" prefHeight="25.0" prefWidth="126.0" promptText="Telephone" />
            <TextField fx:id="address" layoutX="163.0" layoutY="194.0" prefHeight="25.0" prefWidth="126.0" promptText="Address" />
