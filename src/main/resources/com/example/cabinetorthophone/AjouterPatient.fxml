                           <image>
                              <Image url="@previous.png" />
                           </image>
                        </ImageView>
                     </children>
                  </Group>
               </children>
            </AnchorPane>
            <Button layoutX="61.0" layoutY="298.0" mnemonicParsing="false" onAction="#Suivant" prefHeight="32.0" prefWidth="108.0" style="-fx-background-color: #425c59;" text="Suivant" textFill="WHITE">
